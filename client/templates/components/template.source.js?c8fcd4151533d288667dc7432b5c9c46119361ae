(function(){
Template.__checkName("components_source");
Template["components_source"] = new Template("Template.components_source", (function() {
  var view = this;
  return HTML.DIV({
    "class": "panel panel-default panel-dapp panel-source"
  }, "\n        ", HTML.DIV({
    "class": "panel-heading"
  }, "Source \n            ", HTML.SPAN("\n                ", HTML.Raw('<input type="text" placeholder="gas _" id="deployGas">'), "\n                \n                ", HTML.Raw('<button id="deploy" class="btn btn-default btn-sm">Deploy <i class="glyphicon glyphicon-download-alt text-success" id="deploy"></i></button>'), "\n                \n                ", HTML.BUTTON({
    "class": "btn btn-default btn-sm",
    id: "auto"
  }, "Auto ", Blaze.If(function() {
    return Spacebars.dataMustache(Spacebars.dot(view.lookup("$"), "Session", "get"), "auto");
  }, function() {
    return HTML.I({
      "class": "glyphicon glyphicon-check text-info"
    });
  }, function() {
    return HTML.I({
      "class": "text-danger glyphicon glyphicon-unchecked"
    });
  })), "                \n                ", Blaze.If(function() {
    return Spacebars.dataMustache(Spacebars.dot(view.lookup("$"), "Session", "get"), "auto");
  }, function() {
    return null;
  }, function() {
    return [ "\n                    ", HTML.BUTTON({
      "class": "btn btn-default btn-sm",
      id: "refresh"
    }, "Compile ", HTML.I({
      "class": "glyphicon glyphicon-refresh text-info"
    })), "\n                " ];
  }), "\n            "), " \n            ", HTML.Raw('<div class="clear"></div>'), "\n        "), "\n        ", Blaze._TemplateWith(function() {
    return {
      docid: Spacebars.call(view.lookup("document")),
      onRender: Spacebars.call(view.lookup("onAce")),
      onConnect: Spacebars.call(view.lookup("setMode")),
      id: Spacebars.call("editor")
    };
  }, function() {
    return Spacebars.include(view.lookupTemplate("sharejsAce"));
  }), "\n    ");
}));

})();
