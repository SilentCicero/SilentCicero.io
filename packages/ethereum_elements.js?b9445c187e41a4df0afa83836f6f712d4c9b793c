                                                                                                       // 37
Template['dapp_modal'].events({                                                                        // 38
    /**                                                                                                // 39
    Hide the modal on click. If the data context has the property "closePath",                         // 40
    it will route to this one instead of going back in the browser history.                            // 41
                                                                                                       // 42
    @event click .dapp-modal-overlay                                                                   // 43
    */                                                                                                 // 44
    'click .dapp-modal-overlay': function(e){                                                          // 45
        // hide the modal                                                                              // 46
        if($(e.target).hasClass('dapp-modal-overlay')) {                                               // 47
                                                                                                       // 48
            if(typeof Router !== 'undefined') {                                                        // 49
                                                                                                       // 50
                if(this.closePath)                                                                     // 51
                    Router.go(this.closePath);                                                         // 52
