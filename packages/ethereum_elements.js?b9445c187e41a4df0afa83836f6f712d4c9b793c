Template['dapp_addressInput'].created = function(){                                                    // 14
                                                                                                       // 15
    // default set to true, to show no error                                                           // 16
    TemplateVar.set('isValid', true);                                                                  // 17
                                                                                                       // 18
    if(this.data.value)                                                                                // 19
        TemplateVar.set('address', this.data.value);                                                   // 20
};                                                                                                     // 21
                                                                                                       // 22
Template['dapp_addressInput'].helpers({                                                                // 23
    /**                                                                                                // 24
    Return the to address                                                                              // 25
                                                                                                       // 26
    @method (address)                                                                                  // 27
    */                                                                                                 // 28
    'address': function(){                                                                             // 29
        var address = TemplateVar.get('address');                                                      // 30
        return (_.isString(address)) ? address.replace('0x','') : '';                                  // 31
    },                                                                                                 // 32
    /**                                                                                                // 33
    Return the to address                                                                              // 34
                                                                                                       // 35
    @method (isValid)                                                                                  // 36
    */                                                                                                 // 37
    'isValid': function(){                                                                             // 38
        return TemplateVar.get('isValid');                                                             // 39
    }                                                                                                  // 40
});                                                                                                    // 41
                                                                                                       // 42
                                                                                                       // 43
Template['dapp_addressInput'].events({                                                                 // 44
    /**                                                                                                // 45
    Set the to address while typing                                                                    // 46
                                                                                                       // 47
    @event input input[name="to"], change input[name="to"]                                             // 48
    */                                                                                                 // 49
    'input input[name="to"], change input[name="to"]': function(e){                                    // 50
        if(EthTools.isAddress(e.currentTarget.value) || _.isEmpty(e.currentTarget.value))              // 51
            TemplateVar.set('isValid', true);                                                          // 52
        else                                                                                           // 53
            TemplateVar.set('isValid', false);                                                         // 54
        TemplateVar.set('address', e.currentTarget.value);                                             // 55
    },                                                                                                 // 56
    /**                                                                                                // 57
    Prevent the identicon from beeing clicked.                                                         // 58
                                                                                                       // 59
    TODO: remove?                                                                                      // 60
                                                                                                       // 61
    @event click a                                                                                     // 62
    */                                                                                                 // 63
    'click a': function(e){                                                                            // 64
        e.preventDefault();                                                                            // 65
    }                                                                                                  // 66
});                                                                                                    // 67
