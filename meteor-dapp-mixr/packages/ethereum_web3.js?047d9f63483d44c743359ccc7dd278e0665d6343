/* jshint ignore:start */                                                                                             // 1
                                                                                                                      // 2
if(typeof web3 === 'undefined') {                                                                                     // 3
    web3 = require('web3');                                                                                           // 4
    BigNumber = require('bignumber.js');                                                                              // 5
